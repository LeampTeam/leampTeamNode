  body
    .jumbotron-fluid.contenedor
      .container-fluid.contenedor
        .row.contenedorheader
          .col-md-2.border-info.border
            a(href='/admin/index')
              .icono
          .col-md-10.border-top.border-rigth.border-bottom.border-info
            .row.contenedor
              .col-md-9.p-4.text-center
                if data
                  h3.bienvenido.text-center.align-middle.d-inline Buen dia #{data.name} #{data.surname}
              .col-md-3(style='padding-top: 10px;')
                if data
                  h5(style='float:right;margin-top:25px') #{data.email}
                  .dropdown(style='margin-left: 5px;margin-right: 5px;cursor: pointer;')
                    #dropdownMenu2(data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                      img.imgRedonda(src="http://127.0.0.1:3000/admin/users/getImage/"+data.img alt="")
                    .dropdown-menu.dropdown-menu-lg-left(aria-labelledby='dropdownMenu2')
                      form#imgfor.dropdown-item(action='http://127.0.0.1:3000/admin/users/upload' enctype='multipart/form-data' method='post')
                        .upload-btn-wrapper(style='cursor: pointer;')
                          | Cambiar Imagen
                          button.boton-standar-rw
                            i.fas.fa-camera
                          input.upload-file-buton(type='file' name='foto' )
                      .dropdown-divider
                      a.dropdown-item(href='/admin/users/logout') Cerrar Sesion
                  //- #dropdownMenuButton.avatar(type='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                  //-   p #{data.email}
                  //-   img(src="http://127.0.0.1:3000/admin/upload/getImage/"+data.img alt="")
                  //- .dropdown-menu(aria-labelledby='dropdownMenuButton')
                  //-   form(action='http://127.0.0.1:3000/admin/upload/upload' enctype='multipart/form-data' method='post')
                  //-     .form-group
                  //-       label(for='exampleFormControlFile1') Cambiar Avatar
                  //-       input#exampleFormControlFile1.form-control-file(type='file' name="foto")
                  //-       button.btn.btn-primary.mb-2(type='submit') Cambiar
                  //-   a.dropdown-item(href='/admin/users/logout') Cerrar Sesion
        .row.contenedorbody
          .col-md-2.border.border-top-0.border-info.p-3
            if data
              ul.nav.flex-column
                li.nav-item.p-3
                  a.nav-link.btn.btn-info(href='/admin/users/listUser') Usuario
                li.nav-item.p-3
                  a.nav-link.btn.btn-info(href='/admin/producto/grilla') Productos
                li.nav-item.p-3
                  a.nav-link.btn.btn-info(href='/admin/combo/grilla') Combo
                li.nav-item.p-3
                  a.nav-link.btn.btn-info(href='/admin/categoria/grilla') Categoria
                li.nav-item.p-3
                  a.nav-link.btn.btn-info(href='/admin/fragancia/grilla') Fragancia
                li.nav-item.p-3
                  a.nav-link.btn.btn-info(href='/admin/marca/grilla') Marca
                li.nav-item.p-3
                  a.nav-link.btn.btn-info(href='/admin/puntera/puntera') Crear Puntera
          .col-md-10
